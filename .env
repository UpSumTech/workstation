eval "$(pyenv init -)"
eval "$(pyenv virtualenv-init -)"
pyenv versions | grep -i '2.7.10' || pyenv install 2.7.10
pyenv virtualenvs | grep -i 'workstation-2.7.10' || pyenv virtualenv 2.7.10 workstation-2.7.10
pyenv activate workstation-2.7.10
[[ -d ".secrets" ]] && for file in .secrets/*.sh; do . "$file"; done
. bin/activate_profile.sh
